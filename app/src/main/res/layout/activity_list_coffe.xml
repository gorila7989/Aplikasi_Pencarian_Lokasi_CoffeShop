            <ImageView
                android:id="@+id/imgqahwa"
                android:layout_width="150dp"
                android:layout_height="120dp"
                android:src="@drawable/qahwa1"
                android:layout_marginLeft="5dp"/>
            <TextView
                android:id="@+id/judul8"
                android:fontFamily="@font/harbringer"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:layout_marginLeft="10dp"
                android:text="Aqhwa Coffee "
                android:textSize="20sp"
                android:textColor="@android:color/black"
                android:layout_toRightOf="@id/imgqahwa"
                />
            <TextView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginTop="5dp"
                android:layout_marginLeft="10dp"
                android:text="Jl. Kalipasir Indah, RT.003/RW.004, Sukasari, Kec. Tangerang, Kota Tangerang, Banten 15118"
                android:layout_toRightOf="@id/imgqahwa"
                android:layout_below="@id/judul8" />
        </RelativeLayout>
    </androidx.cardview.widget.CardView>


    <androidx.cardview.widget.CardView
        android:id="@+id/tekofio"
        android:clickable="true"
        android:focusable="true"
        android:foreground="?android:attr/selectableItemBackground"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="5dp"
        android:layout_marginRight="5dp"
        android:layout_marginTop="10dp"
        android:layout_below="@id/qahwa">
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">
            <ImageView
                android:id="@+id/imgtekofio"
                android:layout_width="150dp"
                android:layout_height="120dp"
                android:src="@drawable/tekofio"
                android:layout_marginLeft="5dp"/>
            <TextView
                android:id="@+id/judul9"
                android:fontFamily="@font/harbringer"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:layout_marginLeft="10dp"
                android:text="Tekofio Coffee"
                android:textSize="20sp"
                android:textColor="@android:color/black"
                android:layout_toRightOf="@id/imgtekofio"
                />
            <TextView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginTop="5dp"
                android:layout_marginLeft="10dp"
                android:text="Jl. Kalipasir Indah, RT.003/RW.004, Sukasari, Kec. Tangerang, Kota Tangerang, Banten 15118"
