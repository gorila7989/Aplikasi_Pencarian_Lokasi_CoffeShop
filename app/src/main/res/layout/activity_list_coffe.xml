                android:layout_height="120dp"
                android:src="@drawable/pkp"
                android:layout_marginLeft="5dp"/>
            <TextView
                android:id="@+id/judul7"
                android:fontFamily="@font/harbringer"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:layout_marginLeft="10dp"
                android:text="PKP Coffee "
                android:textSize="20sp"
                android:textColor="@android:color/black"
                android:layout_toRightOf="@id/imgpkp"
                />
            <TextView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginTop="5dp"
                android:layout_marginLeft="10dp"
                android:text="Jl. Kalipasir Indah, RT.003/RW.004, Sukasari, Kec. Tangerang, Kota Tangerang, Banten 15118"
                android:layout_toRightOf="@id/imgpkp"
